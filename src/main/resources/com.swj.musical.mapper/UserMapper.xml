<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.swj.musical.mapper.UserMapper">

    <insert id="insertSong">
        insert into user(username,password,token,tk,ck)
        values(#{username},#{password},#{token},#{tk},#{ck})
    </insert>

</mapper>